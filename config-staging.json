{
  "locals": {
    "url": "http://localhost:8080",
    "author": "David Tucker",
    "siteName": "The Site of David Tucker",
    "tagline": "David Tucker is a software architect, digital strategist, speaker, and writer.",
    "dateDescriptionFormat": "dddd, MMMM Do YYYY",
    "defaultOGImage" : "/images/icons/siteIcon-144.png"
  },
  "templates": "./templates",
  "contents": "./contents",
  "output": "./build",
  "require": [
  	"moment",
    "./node_modules/contentHelpers/contentHelpers"
  ],
  "plugins": [
  	"wintersmith-nunjucks"
  ],
  "ignore": [
  	"**/config.rb",
  	"**/sass",
  	"**/.sass-cache",
    "**/.DS_Store"
  ]
}
